    "build": "tsc -p tsconfig.json",
    "start": "npm run build && node ../../dist/supervisor/bin/supervisor.js ",
    "stop": "./stop-superviosor.sh"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@scramjet/types": "file:../types",
    "@types/node": "^14.14.25",
    "ava": "^3.15.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
