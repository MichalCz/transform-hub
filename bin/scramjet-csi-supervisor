#!/usr/bin/env node

const verbose = [...process.argv]
    .slice(2)
    .find(val => ['-v', '--verbose'].includes(val))
;

console.error('verbose', verbose)

require("../dist/supervisor/index")
    .supervisor()
    .catch(e => {
        console.error(e.stack);
        process.exitCode = e.exitCode || 10;
    });