{
  "name": "@scramjet/supervisor",
  "version": "0.2.0",
  "description": "The supervisor application for the underlying container and program wrapper executed within it.",
  "main": "./src/index.ts",
  "scripts": {
    "test": "npm i && npm run test:ava",
    "test:ava": "ava",
    "build:packages": "tsc -p tsconfig.json",
    "start": "npm run build && node ../../dist/supervisor/bin/supervisor.js ",
    "stop": "./stop-superviosor.sh",
    "watch": "tsc -b --watch",
    "prepack": "node ../../scripts/publish.js",
    "clean": "rimraf -rf ./dist"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@scramjet/types": "0.2.0",
    "@types/node": "^14.14.25",
    "ava": "^3.15.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "rimraf": "^3.0.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "scramjet": "^4.35.12"
  }
}
