{
  "name": "@scramjet/logger",
  "version": "0.10.0-pre.9",
  "description": "Important basic logger",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "watch": "tsc -b --watch",
    "test": "npm run test:ava",
    "test:ava": "nyc ava",
    "clean": "rm -rf ./dist",
    "prepack": "node ../../scripts/publish.js"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@scramjet/types": "^0.10.0-pre.9",
    "@types/node": "^14.14.22",
    "ava": "^3.15.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.24",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "scramjet": "4.35.15"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "registry": "https://repo.int.scp.ovh/repository/npm-scramjet/",
    "access": "restricted"
  }
}
