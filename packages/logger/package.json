{
  "name": "@scramjet/logger",
  "version": "0.7.0-pre.1",
  "description": "Important basic logger",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -b --watch",
    "test": "npm run test:ava",
    "test:ava": "ava",
    "prepack": "LOCAL_PACKAGES=true node ../../scripts/publish.js"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.14.22",
    "ava": "^3.15.0",
    "scramjet": "^4.35.15",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.24",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "registry": "https://repo.int.scp.ovh/repository/npm-scramjet/",
    "access": "restricted"
  }
}
