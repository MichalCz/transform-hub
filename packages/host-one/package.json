{
  "name": "@scramjet/host-one",
  "version": "0.2.0",
  "description": "Host one",
  "main": "./src/index.ts",
  "bin": "bin/start-host-one.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && chmod +x ./dist/bin/start-host-one.js",
    "start": "ts-node ./src/index",
    "watch": "tsc -b --watch",
    "test": "nyc ava",
    "clean": "rimraf -rf ./dist",
    "prepack": "LOCAL_PACKAGES=true node ../../scripts/publish.js"
  },
  "author": "",
  "license": "AGPL-3.0",
  "dependencies": {
    "@scramjet/types": "^0.7.0-pre.1",
    "@scramjet/model": "^0.7.0-pre.1",
    "@scramjet/api-server": "^0.7.0-pre.1",
    "bpmux": "^8.1.3",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/sinon": "^9.0.10",
    "@types/vorpal": "^1.12.0",
    "ava": "^3.15.0",
    "nyc": "^15.1.0",
    "typescript": "^4.1.3",
    "proxyquire": "^2.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "registry": "https://repo.int.scp.ovh/repository/npm-scramjet/",
    "access": "restricted"
  }
}
