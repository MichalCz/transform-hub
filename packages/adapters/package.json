{
  "name": "@scramjet/adapters",
  "version": "0.10.0-pre.7",
  "description": "Important model of the scramjet platform",
  "main": "./src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "clean": "rm -rf ./dist",
    "watch": "tsc -b --watch",
    "test": "nyc ava",
    "prepack": "node ../../scripts/publish.js"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@scramjet/csi-config": "^0.10.0-pre.7",
    "@scramjet/logger": "^0.10.0-pre.7",
    "@scramjet/model": "^0.10.0-pre.7",
    "@scramjet/symbols": "^0.10.0-pre.7",
    "dockerode": "^3.3.0",
    "scramjet": "^4.35.15",
    "shell-escape": "^0.2.0"
  },
  "devDependencies": {
    "@scramjet/types": "^0.10.0-pre.7",
    "@types/node": "14.14.33",
    "ava": "^3.15.0",
    "ts-node": "^9.1.1",
    "typedoc": "0.20.28",
    "typedoc-plugin-markdown": "3.4.5",
    "typescript": "4.1.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "registry": "https://repo.int.scp.ovh/repository/npm-scramjet/",
    "access": "restricted"
  }
}
