{
  "name": "@scramjet/host",
  "version": "0.10.0-pre.7",
  "description": "Host",
  "---remove-next-line": "",
  "bic": false,
  "main": "./src/index.ts",
  "bin": {
    "scramjet-host": "bin/start.ts"
  },
  "scripts": {
    "start": "ts-node ./src/index",
    "build": "tsc -p tsconfig.build.json",
    "build:docker": "docker build -t repo.int.scp.ovh/scramjet/host:$npm_package_version -f Dockerfile ../../",
    "build:docker-release": "docker build -t repo.int.scp.ovh/scramjet/host:$npm_package_version -f Dockerfile.release .",
    "build:docs": "typedoc",
    "clean": "rm -rf ./dist",
    "watch": "tsc -b --watch",
    "test": "npm run test:ava",
    "test:ava": "ava",
    "prepack": "node ../../scripts/publish.js"
  },
  "author": "Scramjet <open-source@scramjet.org>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@scramjet/adapters": "^0.10.0-pre.7",
    "@scramjet/api-server": "^0.10.0-pre.7",
    "@scramjet/logger": "^0.10.0-pre.7",
    "@scramjet/model": "^0.10.0-pre.7",
    "@scramjet/supervisor": "^0.10.0-pre.7",
    "@scramjet/symbols": "^0.10.0-pre.7",
    "bpmux": "^8.1.3",
    "http-status-codes": "^2.1.4",
    "minimist": "^1.2.5",
    "scramjet": "^4.35.17",
    "systeminformation": "5.7.2"
  },
  "devDependencies": {
    "@scramjet/types": "^0.10.0-pre.7",
    "@types/node": "14.14.33",
    "ava": "^3.15.0",
    "ts-node": "^9.1.1",
    "typedoc": "0.20.28",
    "typedoc-plugin-markdown": "3.4.5",
    "typescript": "4.1.4"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "registry": "https://repo.int.scp.ovh/repository/npm-scramjet/",
    "access": "restricted"
  }
}
