{
  "name": "@scramjet/runner",
  "version": "0.10.0-pre.7",
  "description": "Runner Mock",
  "main": "./src/",
  "bin": "bin/start-runner.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:docker": "docker build -t repo.int.scp.ovh/scramjet/runner:$npm_package_version -f Dockerfile ../../",
    "build:docker-fast": "docker build -t repo.int.scp.ovh/scramjet/runner:$npm_package_version -f Dockerfile.fast ../../",
    "build:docs": "typedoc",
    "start": "ts-node ./src/index",
    "watch": "tsc -b --watch",
    "test": "nyc ava",
    "test:docker": "docker run -i --name runner -v prerunner-$(git rev-parse --short HEAD):/package scramjet/runner:$npm_package_version",
    "clean": "rm -rf ./dist",
    "clean:docker": "docker rm -f runner; docker volume rm prerunner-$(git rev-parse --short HEAD)",
    "prepack": "node ../../scripts/publish.js"
  },
  "author": "",
  "license": "AGPL-3.0",
  "dependencies": {
    "@scramjet/logger": "^0.10.0-pre.7",
    "@scramjet/model": "^0.10.0-pre.7",
    "@scramjet/symbols": "^0.10.0-pre.7",
    "scramjet": "^4.35.15"
  },
  "devDependencies": {
    "@scramjet/types": "^0.10.0-pre.7",
    "@types/node": "14.14.33",
    "@types/sinon": "^9.0.10",
    "ava": "^3.15.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.4",
    "ts-node": "^9.1.1",
    "typescript": "4.1.4"
  },
  "ava": {
    "extensions": [
      "ts",
      "js"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "registry": "https://repo.int.scp.ovh/repository/npm-scramjet/",
    "access": "restricted"
  }
}
