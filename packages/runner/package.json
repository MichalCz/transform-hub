{
  "name": "@scramjet/runner",
  "private": true,
  "version": "0.2.0",
  "description": "Runner Mock",
  "main": "./src/index.ts",
  "scripts": {
    "build:docker": "./docker-build.sh",
    "start:docker": "docker run -i --rm -v PACKAGE:/package scramjet/runner:latest -t",
    "start": "ts-node ./src/index",
    "build:packages": "tsc -p tsconfig.json",
    "watch": "tsc -b --watch",
    "test": "ava",
    "test-coverage": "nyc ava",
    "clean": "rimraf -rf ./dist",
    "prepack": "node ../../scripts/publish.js"

  },
  "author": "",
  "license": "AGPL-3.0",
  "dependencies": {
    "scramjet": "^4.35.11",
    "@scramjet/model": "0.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@scramjet/types": "0.2.0",
    "ava": "^3.15.0",
    "ninos": "^3.0.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "**/*.spec.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
